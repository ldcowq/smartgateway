    307           * @fn          zmain_dev_info
    308           *
    309           * @brief       This displays the IEEE (MSB to LSB) on the LCD.
    310           *
    311           * input parameters
    312           *
    313           * None.
    314           *
    315           * output parameters
    316           *
    317           * None.
    318           *
    319           * @return      None.
    320           **************************************************************************************************
    321           */
    322          static void zmain_dev_info(void)
    323          {
    324          #ifdef LCD_SUPPORTED
    325            uint8 i;
    326            uint8 *xad;
    327            uint8 lcd_buf[Z_EXTADDR_LEN*2+1];
    328          
    329            // Display the extended address.
    330            xad = aExtendedAddress + Z_EXTADDR_LEN - 1;
    331          
    332            for (i = 0; i < Z_EXTADDR_LEN*2; xad--)
    333            {
    334              uint8 ch;
    335              ch = (*xad >> 4) & 0x0F;
    336              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    337              ch = *xad & 0x0F;
    338              lcd_buf[i++] = ch + (( ch < 10 ) ? '0' : '7');
    339            }
    340            lcd_buf[Z_EXTADDR_LEN*2] = '\0';
    341            HalLcdWriteString( "IEEE: ", HAL_LCD_LINE_1 );
    342            HalLcdWriteString( (char*)lcd_buf, HAL_LCD_LINE_2 );
    343          #endif
    344          }
